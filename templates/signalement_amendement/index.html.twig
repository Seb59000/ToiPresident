{% extends 'base.html.twig' %}

{% block title %}SignalementAmendement index
{% endblock %}

{% block body %}
	<h1>SignalementAmendement index</h1>

	<table class="table">
		<thead>
			<tr>
				<th>Message</th>
				<th>actions</th>
			</tr>
		</thead>
		<tbody>
			{% for signalement_amendement in signalement_amendements %}
				<tr>
					<td>{{ signalement_amendement.message }}</td>
					<td>
						<a href="{{ path('signalement_amendement_show', {'id': signalement_amendement.id}) }}">show</a>
					</td>
				</tr>
			{% else %}
				<tr>
					<td colspan="3">no records found</td>
				</tr>
			{% endfor %}
		</tbody>
	</table>
{% endblock %}
